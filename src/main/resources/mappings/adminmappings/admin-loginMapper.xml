<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.spring.hotelspace.admin.login.dao.AdminLoginDAO">

	<select id="getAdminLoginResult" parameterType="adminLoginVO" resultType="adminLoginVO">
		SELECT 
		*
		FROM ADMIN 
		WHERE id = #{id}
	</select>

</mapper>
